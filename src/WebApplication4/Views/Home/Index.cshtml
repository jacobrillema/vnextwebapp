@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET vNext</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net/vnext" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-6">
        <h2>This application consists of:</h2>
        <ul>
            <li>Application Name = @ViewBag.ApplicationName</li>
            <li>Application Version = @ViewBag.ApplicationVersion</li>
            <li>KRE Version = @ViewBag.KREVersion</li>
            <li>KRE Framework = @ViewBag.KREFrameworkName</li>
            <li>KRE AppBase = @ViewBag.KREAppBase</li>
            <li>Configuration = @ViewBag.Configuration</li>
        </ul>
    </div>
</div>